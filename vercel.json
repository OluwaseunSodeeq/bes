{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "build/$1"
    }
  ]
}
